package infoalumnos;

public class Nota 
{
	private String asignatura;
	private double[] nota;
	
	public Nota(String asignaturas,double[]notas)
	{
		this.asignatura=asignatura;
		this.nota=nota;
	}
	public Nota()
	{
		this.asignatura="";
		this.nota=null;
	}

	public String getAsignatura() {
		return asignatura;
	}

	public void setAsignatura(String asignatura) {
		this.asignatura = asignatura;
	}

	public double[] getNota() {
		return nota;
	}

	public void setNota(double[] nota) {
		this.nota = nota;
	}
}
